<com.zhy.autolayout.AutoLinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"

    android:orientation="vertical"
    >

    <com.zhy.autolayout.AutoLinearLayout
        android:layout_width="match_parent"
        android:layout_height="96px"
        android:background="@drawable/bg2"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/register_main"
            android:layout_width="140px"
            android:layout_height="37px"
            android:gravity="center"
            android:layout_marginLeft="305px"
            android:textSize="35px"
            android:textColor="@color/white"
            android:layout_marginTop="27px"
            android:text="@string/cust_collection"
            tools:ignore="RtlHardcoded"/>
    </com.zhy.autolayout.AutoLinearLayout>

    <com.zhy.autolayout.AutoLinearLayout
        android:layout_width="match_parent"
        android:layout_height="102px"
        android:background="@drawable/bg2"
        android:orientation="horizontal"
        >
        <com.zhy.autolayout.AutoLinearLayout
            android:layout_width="720px"
            android:layout_height="68px"
            android:layout_marginLeft="15px"
            android:layout_marginTop="16px"
            android:background="@drawable/shape_34">
            <Button
                android:layout_width="157px"
                android:layout_height="68px"
                android:text="@string/collection"
                android:textSize="30px"
                android:id="@+id/Collection"
                android:background="@drawable/shape_button34"
                android:textColor="#ffffff"/>

            <TextView
                android:layout_width="1px"
                android:layout_height="38px"
                android:layout_marginTop="10px"
                android:background="#20ffffff"/>

            <Button
                android:layout_width="191px"
                android:layout_height="68px"
                android:text="@string/contact"
                android:id="@+id/contact"
                android:textSize="30px"
                android:background="#00ffffff"
                android:textColor="#50ffffff"/>

            <TextView
                android:layout_width="1px"
                android:layout_height="38px"
                android:layout_marginTop="10px"
                android:background="#20ffffff"/>
            <Button
                android:layout_width="188px"
                android:layout_height="66px"
                android:text="@string/ok_contact"
                android:id="@+id/ok_contact"
                android:textSize="30px"
                android:layout_marginTop="1px"
                android:layout_marginLeft="1px"
                android:background="#00ffffff"
                android:textColor="#50ffffff"/>
            <TextView
                android:layout_width="1px"
                android:layout_height="38px"
                android:layout_marginTop="10px"
                android:background="#20ffffff"/>
            <Button
                android:layout_width="match_parent"
                android:layout_height="66px"
                android:text="已查看"
                android:id="@+id/Already_view"
                android:textSize="30px"
                android:layout_marginTop="1px"
                android:layout_marginLeft="1px"
                android:background="#00ffffff"
                android:textColor="#50ffffff"/>
        </com.zhy.autolayout.AutoLinearLayout>
    </com.zhy.autolayout.AutoLinearLayout>
    <com.example.dm_skb.widget.PullToRefreshListView
        xmlns:ptr="http://schemas.android.com/apk/res-auto"
        android:id="@+id/Collection_list"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        ptr:ptrDrawable="@drawable/pull_down_icon"
        ptr:ptrAnimationStyle="flip"
        android:scrollbars="none"
        ptr:ptrHeaderBackground="@color/white"
        ptr:ptrHeaderTextColor="#000000"
        />
    <com.zhy.autolayout.AutoLinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:visibility="gone"
        android:id="@+id/contact_layout"
        >
        <com.zhy.autolayout.AutoLinearLayout
            android:layout_width="749px"
            android:layout_height="71px"
            android:background="#f3f3f3"
            android:orientation="horizontal"
            android:id="@+id/stay_in_touch_today"
            >
            <ImageView
                android:layout_width="23px"
                android:layout_height="24px"
                android:layout_marginLeft="32px"
                android:layout_marginTop="23px"
                android:background="@drawable/date09"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="26px"
                android:layout_marginLeft="22px"
                android:layout_marginTop="20px"
                android:textColor="#7c7c7c"
                android:text="@string/stay_in_touch_today"
                android:textSize="24px"/>
        </com.zhy.autolayout.AutoLinearLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="2px"
            android:background="#aaaaaa"/>
        <com.example.dm_skb.widget.PullToRefreshListView
            xmlns:ptr="http://schemas.android.com/apk/res-auto"
            android:id="@+id/stay_in_touch_today_list"
            android:layout_width="wrap_content"
            android:layout_height="700px"
            ptr:ptrDrawable="@drawable/pull_down_icon"
            ptr:ptrAnimationStyle="flip"
            android:scrollbars="none"
            ptr:ptrHeaderBackground="@color/white"
            ptr:ptrHeaderTextColor="#000000"
            />

        <com.zhy.autolayout.AutoLinearLayout
            android:layout_width="749px"
            android:layout_height="71px"
            android:background="#f3f3f3"
            android:orientation="horizontal"
            android:id="@+id/to_be_contacted_tomorrow"
            >
            <ImageView
                android:layout_width="23px"
                android:layout_height="24px"
                android:layout_marginLeft="32px"
                android:layout_marginTop="23px"
                android:background="@drawable/date09"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="26px"
                android:layout_marginLeft="22px"
                android:layout_marginTop="20px"
                android:text="@string/to_be_contacted_tomorrow"
                android:textColor="#7c7c7c"
                android:textSize="24px"/>
        </com.zhy.autolayout.AutoLinearLayout>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="2px"
            android:background="#aaaaaa"/>
        <com.example.dm_skb.widget.PullToRefreshListView
            xmlns:ptr="http://schemas.android.com/apk/res-auto"
            android:id="@+id/to_be_contacted_tomorrow_list"
            android:layout_width="match_parent"
            android:layout_height="700px"
            ptr:ptrDrawable="@drawable/pull_down_icon"
            ptr:ptrAnimationStyle="flip"
            android:scrollbars="none"
            ptr:ptrHeaderBackground="@color/white"
            ptr:ptrHeaderTextColor="#000000"
            />
        <com.zhy.autolayout.AutoLinearLayout
            android:layout_width="749px"
            android:layout_height="71px"
            android:background="#f3f3f3"
            android:orientation="horizontal"
            android:id="@+id/history_to_be_linked"
            >
            <ImageView
                android:layout_width="23px"
                android:layout_height="24px"
                android:layout_marginLeft="32px"
                android:layout_marginTop="23px"
                android:background="@drawable/date09"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="26px"
                android:layout_marginLeft="22px"
                android:layout_marginTop="20px"
                android:textColor="#7c7c7c"
                android:text="@string/history_to_be_linked"
                android:textSize="24px"/>
        </com.zhy.autolayout.AutoLinearLayout>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="2px"
            android:background="#aaaaaa"/>
        <com.example.dm_skb.widget.PullToRefreshListView
            xmlns:ptr="http://schemas.android.com/apk/res-auto"
            android:id="@+id/history_to_be_linked_list"
            android:layout_width="match_parent"
            android:layout_height="700px"
            ptr:ptrDrawable="@drawable/pull_down_icon"
            ptr:ptrAnimationStyle="flip"
            android:scrollbars="none"
            ptr:ptrHeaderBackground="@color/white"
            ptr:ptrHeaderTextColor="#000000"
            />
        <com.zhy.autolayout.AutoLinearLayout
            android:layout_width="749px"
            android:layout_height="71px"
            android:background="#f3f3f3"
            android:orientation="horizontal"
            android:id="@+id/future_to_be_contacted"
            >
            <ImageView
                android:layout_width="23px"
                android:layout_height="24px"
                android:layout_marginLeft="32px"
                android:layout_marginTop="23px"
                android:background="@drawable/date09"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="26px"
                android:layout_marginLeft="22px"
                android:layout_marginTop="20px"
                android:textColor="#7c7c7c"
                android:text="@string/future_to_be_contacted"
                android:textSize="24px"/>
        </com.zhy.autolayout.AutoLinearLayout>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="2px"
            android:background="#aaaaaa"/>
        <com.example.dm_skb.widget.PullToRefreshListView
            xmlns:ptr="http://schemas.android.com/apk/res-auto"
            android:id="@+id/future_to_be_contacted_list"
            android:layout_width="match_parent"
            android:layout_height="700px"
            ptr:ptrDrawable="@drawable/pull_down_icon"
            ptr:ptrAnimationStyle="flip"
            android:scrollbars="none"
            ptr:ptrHeaderBackground="@color/white"
            ptr:ptrHeaderTextColor="#000000"
            />
    </com.zhy.autolayout.AutoLinearLayout>
    <com.zhy.autolayout.AutoLinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:visibility="gone"
        android:id="@+id/ok_contact_layout"
        >
        <com.zhy.autolayout.AutoLinearLayout
            android:layout_width="749px"
            android:layout_height="71px"
            android:background="#f3f3f3"
            android:orientation="horizontal"
            android:id="@+id/stay_in_touch_today_ok"
            >
            <ImageView
                android:layout_width="23px"
                android:layout_height="24px"
                android:layout_marginLeft="32px"
                android:layout_marginTop="23px"
                android:background="@drawable/date09"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="26px"
                android:layout_marginLeft="22px"
                android:layout_marginTop="20px"
                android:textColor="#7c7c7c"
                android:text="@string/stay_in_touch_today_ok"
                android:textSize="24px"/>
        </com.zhy.autolayout.AutoLinearLayout>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="2px"
            android:background="#aaaaaa"/>
        <com.zhy.autolayout.AutoLinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <com.example.dm_skb.widget.PullToRefreshListView
                xmlns:ptr="http://schemas.android.com/apk/res-auto"
                android:id="@+id/stay_in_touch_today_ok_list"
                android:layout_width="match_parent"
                android:layout_height="630px"
                ptr:ptrDrawable="@drawable/pull_down_icon"
                ptr:ptrAnimationStyle="flip"
                android:scrollbars="none"
                ptr:ptrHeaderBackground="@color/white"
                ptr:ptrHeaderTextColor="#000000"
                />
        </com.zhy.autolayout.AutoLinearLayout>


        <com.zhy.autolayout.AutoLinearLayout
            android:layout_width="749px"
            android:layout_height="71px"
            android:background="#f3f3f3"
            android:orientation="horizontal"
            android:id="@+id/to_be_contacted_tomorrow_ok"
            >
            <ImageView
                android:layout_width="23px"
                android:layout_height="24px"
                android:layout_marginLeft="32px"
                android:layout_marginTop="23px"
                android:background="@drawable/date09"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="26px"
                android:layout_marginLeft="22px"
                android:layout_marginTop="20px"
                android:text="昨天已联系"
                android:textColor="#7c7c7c"
                android:textSize="24px"/>
        </com.zhy.autolayout.AutoLinearLayout>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="2px"
            android:background="#aaaaaa"/>
        <com.example.dm_skb.widget.PullToRefreshListView
            xmlns:ptr="http://schemas.android.com/apk/res-auto"
            android:id="@+id/to_be_contacted_tomorrow_list_ok"
            android:layout_width="match_parent"
            android:layout_height="630px"
            ptr:ptrDrawable="@drawable/pull_down_icon"
            ptr:ptrAnimationStyle="flip"
            android:scrollbars="none"
            ptr:ptrHeaderBackground="@color/white"
            ptr:ptrHeaderTextColor="#000000"
            />
        <com.zhy.autolayout.AutoLinearLayout
            android:layout_width="749px"
            android:layout_height="71px"
            android:background="#f3f3f3"
            android:orientation="horizontal"
            android:id="@+id/history_to_be_linked_ok"
            >
            <ImageView
                android:layout_width="23px"
                android:layout_height="24px"
                android:layout_marginLeft="32px"
                android:layout_marginTop="23px"
                android:background="@drawable/date09"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="26px"
                android:layout_marginLeft="22px"
                android:layout_marginTop="20px"
                android:textColor="#7c7c7c"
                android:text="一周前已联系"
                android:textSize="24px"/>
        </com.zhy.autolayout.AutoLinearLayout>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="2px"
            android:background="#aaaaaa"/>
        <com.example.dm_skb.widget.PullToRefreshListView
            xmlns:ptr="http://schemas.android.com/apk/res-auto"
            android:id="@+id/history_to_be_linked_list_ok"
            android:layout_width="match_parent"
            android:layout_height="630px"
            ptr:ptrDrawable="@drawable/pull_down_icon"
            ptr:ptrAnimationStyle="flip"
            android:scrollbars="none"
            ptr:ptrHeaderBackground="@color/white"
            ptr:ptrHeaderTextColor="#000000"
            />
        <com.zhy.autolayout.AutoLinearLayout
            android:layout_width="749px"
            android:layout_height="71px"
            android:background="#f3f3f3"
            android:orientation="horizontal"
            android:id="@+id/future_to_be_contacted_ok"
            >
            <ImageView
                android:layout_width="23px"
                android:layout_height="24px"
                android:layout_marginLeft="32px"
                android:layout_marginTop="23px"
                android:background="@drawable/date09"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="26px"
                android:layout_marginLeft="22px"
                android:layout_marginTop="20px"
                android:textColor="#7c7c7c"
                android:text="一个月内已联系"
                android:textSize="24px"/>
        </com.zhy.autolayout.AutoLinearLayout>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="2px"
            android:background="#aaaaaa"/>
        <com.example.dm_skb.widget.PullToRefreshListView
            xmlns:ptr="http://schemas.android.com/apk/res-auto"
            android:id="@+id/future_to_be_contacted_list_ok"
            android:layout_width="match_parent"
            android:layout_height="630px"
            ptr:ptrDrawable="@drawable/pull_down_icon"
            ptr:ptrAnimationStyle="flip"
            android:scrollbars="none"
            ptr:ptrHeaderBackground="@color/white"
            ptr:ptrHeaderTextColor="#000000"
            />
        <com.zhy.autolayout.AutoLinearLayout
            android:layout_width="749px"
            android:layout_height="71px"
            android:background="#f3f3f3"
            android:orientation="horizontal"
            android:id="@+id/months_ago"
            >
            <ImageView
                android:layout_width="23px"
                android:layout_height="24px"
                android:layout_marginLeft="32px"
                android:layout_marginTop="23px"
                android:background="@drawable/date09"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="26px"
                android:layout_marginLeft="22px"
                android:layout_marginTop="20px"
                android:textColor="#7c7c7c"
                android:text="一个月前已联系"
                android:textSize="24px"/>
        </com.zhy.autolayout.AutoLinearLayout>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="2px"
            android:background="#aaaaaa"/>
        <com.example.dm_skb.widget.PullToRefreshListView
            xmlns:ptr="http://schemas.android.com/apk/res-auto"
            android:id="@+id/months_ago_list"
            android:layout_width="match_parent"
            android:layout_height="630px"
            ptr:ptrDrawable="@drawable/pull_down_icon"
            ptr:ptrAnimationStyle="flip"
            android:scrollbars="none"
            ptr:ptrHeaderBackground="@color/white"
            ptr:ptrHeaderTextColor="#000000"
            />
    </com.zhy.autolayout.AutoLinearLayout>
    <com.example.dm_skb.widget.PullToRefreshListView
        xmlns:ptr="http://schemas.android.com/apk/res-auto"
        android:id="@+id/getMySeeCompanyList_list"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        ptr:ptrDrawable="@drawable/pull_down_icon"
        ptr:ptrAnimationStyle="flip"
        android:scrollbars="none"
        ptr:ptrHeaderBackground="@color/white"
        ptr:ptrHeaderTextColor="#000000"
        />
</com.zhy.autolayout.AutoLinearLayout>





