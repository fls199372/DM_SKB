<com.zhy.autolayout.AutoLinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical"
    >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="96px"
        android:background="@drawable/bg2"
        android:orientation="horizontal">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:onClick="onClickAuth"
            android:id="@+id/Return"
            >
            <ImageView
                android:layout_width="18px"
                android:layout_height="35px"
                android:layout_marginLeft="30px"
                android:layout_marginTop="30px"
                android:background="@drawable/arrow_left_icon"/>
            <TextView
                android:layout_width="70px"
                android:layout_height="37px"
                android:layout_marginTop="27px"
                android:textSize="35px"
                android:textColor="@color/white"
                android:layout_marginLeft="13px"
                android:text="@string/Return"/>
        </LinearLayout>
        <TextView
            android:id="@+id/register_main"
            android:layout_width="284px"
            android:layout_height="37px"
            android:gravity="center"
            android:layout_marginLeft="102px"
            android:textSize="35px"
            android:textColor="@color/white"
            android:layout_marginTop="27px"
            android:text="@string/errorcorrection" />
        <TextView
            android:layout_width="120px"
            android:layout_height="37px"
            android:layout_marginTop="27px"
            android:layout_marginLeft="110px"
            android:textColor="@color/white"
            android:textSize="35px"
            android:id="@+id/ok"
            android:text="拍名片"
            android:onClick="onClickAuth"
            />
    </LinearLayout>

    <ScrollView
        android:layout_height="wrap_content"
        android:id="@+id/scrollView1"
        android:layout_width="match_parent"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
    <TextView
        android:layout_width="406px"
        android:layout_height="37px"
        android:layout_marginLeft="172px"
        android:gravity="center"

        android:layout_marginTop="55px"
        android:text="@string/error_correction_cemo"
        android:textSize="35px"
        android:textColor="#000000"/>
    <TextView
        android:layout_width="630px"
        android:layout_height="88px"
        android:layout_marginLeft="60px"
        android:layout_marginTop="33px"
        android:gravity="center"
        android:text="@string/error_correction_cem1"
        android:textSize="28px"
        android:textColor="#69000000"/>
    <TextView
        android:layout_width="418px"
        android:layout_height="26px"
        android:layout_marginLeft="166px"
        android:layout_marginTop="21px"
        android:gravity="center"
        android:text="@string/error_correction_cem2"
        android:textSize="24px"
        android:textColor="#ffa200"/>
            <TextView
                android:layout_width="630px"
                android:layout_height="88px"
                android:layout_marginLeft="60px"
                android:layout_marginTop="5px"

                android:gravity="center"
                android:text="您可以选择填写如下纠错项目，也可以点击右上角'拍名片'，拍摄上传该客户的名片"
                android:textSize="28px"
                android:textColor="#69000000"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="129px"
        android:layout_marginTop="24px"
        android:orientation="horizontal"
        >
        <TextView
            android:id="@+id/without_this_customer"
            android:layout_width="20px"
            android:layout_height="20px"
            android:layout_marginLeft="50px"
            android:background="@drawable/shape_circular"
            android:layout_marginTop="53px"
            />
        <TextView
            android:layout_width="130px"
            android:layout_height="34px"
            android:layout_marginLeft="40px"
            android:layout_marginTop="41px"
            android:textColor="#1d1d26"
            android:textSize="32px"
            android:text="@string/without_this_customer"/>
        <ImageView
            android:layout_width="60px"
            android:layout_height="60px"
            android:layout_marginLeft="410px"
            android:id="@+id/without_this_customer_image"
            android:onClick="onClickAuth"
            android:layout_marginTop="41px"
            android:background="@drawable/add1"/>
    </LinearLayout>
    <TextView
        android:layout_width="match_parent"
        android:id="@+id/without_this_customer_Line"
        android:layout_height="4px"
        android:background="#051d1d26"/>
    <EditText
        android:layout_width="596px"
        android:layout_height="210px"
        android:layout_marginLeft="113px"
        android:background="@drawable/shape_prompt"
        android:textSize="24px"
        android:paddingTop="15px"
        android:paddingLeft="5px"
        android:gravity="top"
        android:visibility="gone"
        android:textColor="#000000"
        android:textColorHint="#999999"
        android:imeOptions="actionDone"
        android:inputType="text"
        android:id="@+id/without_this_customer_edittext"
        android:hint="@string/feedback"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="129px"
                android:layout_marginTop="24px"
                android:orientation="horizontal"
                >
                <TextView
                    android:id="@+id/Contacts_error"
                    android:layout_width="20px"
                    android:layout_height="20px"
                    android:layout_marginLeft="50px"
                    android:background="@drawable/shape_circular"
                    android:layout_marginTop="53px"
                    />
                <TextView
                    android:layout_width="260px"
                    android:layout_height="34px"
                    android:layout_marginLeft="40px"
                    android:layout_marginTop="41px"
                    android:textColor="#1d1d26"
                    android:textSize="32px"
                    android:text="@string/Contacts_error"/>
                <ImageView
                    android:layout_width="60px"
                    android:layout_height="60px"
                    android:layout_marginLeft="280px"
                    android:layout_marginTop="41px"
                    android:onClick="onClickAuth"
                    android:id="@+id/Contacts_error_image"
                    android:background="@drawable/add1"/>
            </LinearLayout>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="4px"
                android:id="@+id/Contacts_error_ling"
                android:background="#051d1d26"/>
            <EditText
                android:layout_width="596px"
                android:layout_height="210px"
                android:layout_marginLeft="113px"
                android:paddingLeft="5px"
                android:gravity="top"
                android:paddingTop="15px"
                android:visibility="gone"
                android:background="@drawable/shape_prompt"
                android:textSize="24px"
                android:textColor="#000000"
                android:textColorHint="#999999"
                android:imeOptions="actionDone"
                android:inputType="text"
                android:id="@+id/Contacts_error_edittext"
                android:hint="@string/feedback"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="30px"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="129px"
        android:layout_marginTop="24px"
        android:orientation="horizontal"
        >
        <TextView
            android:id="@+id/channel_no"
            android:layout_width="20px"
            android:layout_height="20px"
            android:layout_marginLeft="50px"
            android:background="@drawable/shape_circular"
            android:layout_marginTop="53px"
            />
        <TextView
            android:layout_width="260px"
            android:layout_height="34px"
            android:layout_marginLeft="40px"
            android:layout_marginTop="41px"
            android:textColor="#1d1d26"
            android:textSize="32px"
            android:text="@string/channel_no"/>
        <ImageView
            android:layout_width="60px"
            android:layout_height="60px"
            android:layout_marginLeft="280px"
            android:layout_marginTop="41px"
            android:id="@+id/channel_no_image"
            android:onClick="onClickAuth"
            android:background="@drawable/add1"/>
    </LinearLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="4px"
        android:id="@+id/channel_no_len"
        android:background="#051d1d26"/>
    <EditText
        android:layout_width="596px"
        android:layout_height="210px"
        android:layout_marginLeft="113px"
        android:background="@drawable/shape_prompt"
        android:textSize="24px"
        android:textColor="#000000"
        android:paddingLeft="5px"
        android:gravity="top"
        android:paddingTop="15px"
        android:visibility="gone"
        android:textColorHint="#999999"
        android:imeOptions="actionDone"
        android:inputType="text"
        android:id="@+id/channel_no_edittext"
        android:hint="@string/feedback"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="129px"
                android:layout_marginTop="24px"
                android:orientation="horizontal"
                >
                <TextView
                    android:id="@+id/phone_error"
                    android:layout_width="20px"
                    android:layout_height="20px"
                    android:layout_marginLeft="50px"
                    android:background="@drawable/shape_circular"
                    android:layout_marginTop="53px"
                    />
                <TextView
                    android:layout_width="325px"
                    android:layout_height="34px"
                    android:layout_marginLeft="40px"
                    android:layout_marginTop="41px"
                    android:textColor="#1d1d26"
                    android:textSize="32px"
                    android:text="@string/phone_error"/>
                <ImageView
                    android:layout_width="60px"
                    android:layout_height="60px"
                    android:layout_marginLeft="215px"
                    android:layout_marginTop="41px"
                    android:onClick="onClickAuth"
                    android:id="@+id/phone_error_image"
                    android:background="@drawable/add1"/>
            </LinearLayout>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="4px"
                android:id="@+id/phone_error_line"
                android:background="#051d1d26"/>
            <EditText
                android:layout_width="596px"
                android:layout_height="210px"
                android:layout_marginLeft="113px"
                android:background="@drawable/shape_prompt"
                android:textSize="24px"
                android:paddingLeft="5px"
                android:gravity="top"
                android:paddingTop="15px"
                android:visibility="gone"
                android:textColor="#000000"
                android:textColorHint="#999999"
                android:imeOptions="actionDone"
                android:inputType="text"
                android:id="@+id/phone_error_edittext"
                android:hint="@string/feedback"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="129px"
        android:layout_marginTop="24px"
        android:orientation="horizontal"
        >
        <TextView
            android:id="@+id/basic_information_error"
            android:layout_width="20px"
            android:layout_height="20px"
            android:layout_marginLeft="50px"
            android:background="@drawable/shape_circular"
            android:layout_marginTop="53px"
            />
        <TextView
            android:layout_width="260px"
            android:layout_height="34px"
            android:layout_marginLeft="40px"
            android:layout_marginTop="41px"
            android:textColor="#1d1d26"
            android:textSize="32px"
            android:text="@string/basic_information_error"/>
        <ImageView
            android:layout_width="60px"
            android:layout_height="60px"
            android:layout_marginLeft="280px"
            android:layout_marginTop="41px"
            android:onClick="onClickAuth"
            android:id="@+id/basic_information_error_image"
            android:background="@drawable/add1"/>
    </LinearLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="4px"
        android:id="@+id/basic_information_error_lien"
        android:background="#051d1d26"/>
    <EditText
        android:layout_width="596px"
        android:layout_height="210px"
        android:layout_marginLeft="113px"
        android:background="@drawable/shape_prompt"
        android:textSize="24px"
        android:textColor="#000000"
        android:textColorHint="#999999"
        android:paddingLeft="5px"
        android:gravity="top"
        android:paddingTop="15px"
        android:visibility="gone"
        android:imeOptions="actionDone"
        android:inputType="text"
        android:id="@+id/basic_information_error_edittext"
        android:hint="@string/feedback"/>



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="129px"
        android:layout_marginTop="24px"
        android:orientation="horizontal"
        >
        <TextView
            android:id="@+id/address_error"
            android:layout_width="20px"
            android:layout_height="20px"
            android:layout_marginLeft="50px"
            android:background="@drawable/shape_circular"
            android:layout_marginTop="53px"
            />
        <TextView
            android:layout_width="260px"
            android:layout_height="34px"
            android:layout_marginLeft="40px"
            android:layout_marginTop="41px"
            android:textColor="#1d1d26"
            android:textSize="32px"
            android:text="@string/address_error"/>
        <ImageView
            android:layout_width="60px"
            android:layout_height="60px"
            android:layout_marginLeft="280px"
            android:layout_marginTop="41px"
            android:onClick="onClickAuth"
            android:id="@+id/address_error_image"
            android:background="@drawable/add1"/>
    </LinearLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="4px"
        android:id="@+id/address_error_line"
        android:background="#051d1d26"/>
    <EditText
        android:layout_width="596px"
        android:layout_height="210px"
        android:layout_marginLeft="113px"
        android:background="@drawable/shape_prompt"
        android:textSize="24px"
        android:paddingLeft="5px"
        android:gravity="top"
        android:paddingTop="15px"
        android:visibility="gone"
        android:textColor="#000000"
        android:textColorHint="#999999"
        android:imeOptions="actionDone"
        android:inputType="text"
        android:id="@+id/address_error_edittext"
        android:hint="@string/feedback"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="129px"
        android:layout_marginTop="24px"
        android:orientation="horizontal"
        >
        <TextView
            android:id="@+id/EMS_error"
            android:layout_width="20px"
            android:layout_height="20px"
            android:layout_marginLeft="50px"
            android:background="@drawable/shape_circular"
            android:layout_marginTop="53px"
            />
        <TextView
            android:layout_width="342px"
            android:layout_height="34px"
            android:layout_marginLeft="40px"
            android:layout_marginTop="41px"
            android:textColor="#1d1d26"
            android:textSize="32px"
            android:text="@string/EMS_error"/>
        <ImageView
            android:layout_width="60px"
            android:layout_height="60px"
            android:layout_marginLeft="198px"
            android:layout_marginTop="41px"
            android:onClick="onClickAuth"
            android:id="@+id/EMS_error_image"
            android:background="@drawable/add1"/>
    </LinearLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="4px"
        android:id="@+id/EMS_error_ling"
        android:background="#051d1d26"/>
    <EditText
        android:layout_width="596px"
        android:layout_height="210px"
        android:layout_marginLeft="113px"
        android:background="@drawable/shape_prompt"
        android:textSize="24px"
        android:paddingLeft="5px"
        android:gravity="top"
        android:paddingTop="15px"
        android:visibility="gone"
        android:textColor="#000000"
        android:textColorHint="#999999"
        android:imeOptions="actionDone"
        android:inputType="text"
        android:id="@+id/EMS_error_edittext"
        android:hint="@string/feedback"/>


        </LinearLayout>
</ScrollView>

</com.zhy.autolayout.AutoLinearLayout>
